{
  "name": "chartjs-plugin-interaction-tools",
  "version": "0.1.0",
  "description": "Two Chart.js plugins: one for dragging points with a trail, and another for drawing freeform trails with the mouse.",
  "main": "dist/chartjs-plugin-interaction-tools.umd.js",
  "module": "dist/chartjs-plugin-interaction-tools.esm.js",
  "exports": {
    "import": "./dist/chartjs-plugin-interaction-tools.esm.js",
    "require": "./dist/chartjs-plugin-interaction-tools.umd.js"
  },
  "keywords": [
    "chartjs",
    "chart.js",
    "plugin",
    "drag",
    "trail",
    "interaction",
    "chart",
    "canvas"
  ],
  "author": "Noemi Vital / Rui Afonso",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "rollup": "^4.41.1"
  },
  "peerDependencies": {
    "chart.js": "^4.0.0"
  }
}
